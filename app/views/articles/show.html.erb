<br>
<div class="container jumbotron">
    <p>
        <strong><%= @article[:title] %></strong>
    </p>

    <p>
        <%= @article[:description] %>
    <p>

    <p>
        <em>Created at: <%= @article[:created_at]%></em>
    </p>
    <p><%= link_to 'All articles', articles_path %></p>
    <%= button_to "Edit", edit_article_path, :method => "get" %>
    <%= button_to "Delete", article_path, :method => "delete", data: { confirm: 'Are you sure?' }%>
</div>
