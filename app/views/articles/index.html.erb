<h1>Articles</h1>
<% if @article_list.any? %>

    <% @article_list.each do |article| %>
        <p>
            Title: <%= article[:title] %>
        </p>

        <p>
            Description: <%= article[:description] %>
        <p>

        <p>
            <em>Created at: <%= article[:created_at]%></em>
        </p>
        <br>
    <% end %>
<% end %>